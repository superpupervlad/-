# =============================================
#     Конец добавления определения параметра      
# =============================================


# =============================================
#     Начало установки значения параметра      
# =============================================


# =============================================
#     Конец установки значения параметра      
# =============================================


Dhcp Server \\s1 Set AuditLog path = "C:\Windows\system32\dhcp" maxsize = 70 minsize = 20 interval = 50 mode = ENABLE
Dhcp Server \\s1 Set Bindings interface = "Ethernet" mode = ENABLE
Dhcp Server \\s1 v4 Set NameProtection enable= 0
Dhcp Server \\s1 Set DnsConfig 1 0 1 0
Dhcp Server \\s1 Set DatabaseBackupInterval 60
Dhcp Server \\s1 Set DatabaseBackupPath "C:\Windows\system32\dhcp\backup"
Dhcp Server \\s1 Set DatabaseCleanupInterval 60
Dhcp Server \\s1 Set DatabaseLoggingFlag 1
Dhcp Server \\s1 Set DatabaseName "dhcp.mdb"
Dhcp Server \\s1 Set DatabasePath "C:\Windows\system32\dhcp"
Dhcp Server \\s1 Set DatabaseRestoreFlag 0
Dhcp Server \\s1 Set DetectConflictRetry 0

# =============================================
#     Начало добавления новой области      
# =============================================

Dhcp Server \\s1 add scope 10.0.0.0 255.0.0.0 "1" "LABA"
Dhcp Server \\s1 Scope 10.0.0.0 set state 1
Dhcp Server \\s1 Scope 10.0.0.0 set delayoffer 0

# ============================================================================
# Начало добавления IP-диапазона
# к области 10.0.0.0, сервера s1
# ============================================================================


Dhcp Server \\s1 Scope 10.0.0.0 Add iprange 10.0.0.100 10.0.0.205 
Dhcp Server \\s1 scope 10.0.0.0 set napstate Off

# ============================================================================
# Конец добавления IP-диапазона
# к области 10.0.0.0, сервера s1
# ============================================================================


# =============================================================================
# Начало добавления исключаемого IP-диапазона
# к области 10.0.0.0, сервера s1
# =============================================================================


Dhcp Server \\s1 Scope 10.0.0.0 add excluderange 10.0.0.195 10.0.0.200

# =============================================================================
# Конец добавления исключаемого IP-диапазона
# к области 10.0.0.0, сервера s1
# =============================================================================


# =============================================================================
# Начало добавления значений параметра
# к области 10.0.0.0, сервера s1
# =============================================================================


Dhcp Server \\s1 Scope 10.0.0.0 set optionvalue 51 DWORD "3600" 
Dhcp Server \\s1 Scope 10.0.0.0 set optionvalue 3 IPADDRESS "10.10.10.10" 
Dhcp Server \\s1 Scope 10.0.0.0 set optionvalue 15 STRING "YVV.loc" 
Dhcp Server \\s1 Scope 10.0.0.0 set optionvalue 6 IPADDRESS "10.10.10.10" 

# =============================================================================
# Конец добавления значений параметра
# к области 10.0.0.0, сервера s1
# =============================================================================


# ============================================================================
# Начало добавления зарезервированных IP-адресов
# к области 10.0.0.0, сервера s1
# ============================================================================


Dhcp Server \\s1 Scope 10.0.0.0 Add reservedip 10.0.0.199 000102030405 "2.6" "" "BOTH"
Dhcp Server \\s1 v4 Scope 10.0.0.0 Set NameProtection enable= 0
Dhcp Server \\s1 Scope 10.0.0.0 Set DnsConfig 1 0 1 0

# ============================================================================
# Конец добавления зарезервированных IP-адресов
# к области 10.0.0.0, сервера s1
# ============================================================================



